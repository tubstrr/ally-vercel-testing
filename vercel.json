{
  "rewrites": [
    {
      "source": "/ally-admin/login",
      "destination": "api/ally-admin/login"
    },
    {
      "source": "/ally-admin/create-account",
      "destination": "api/ally-admin/create-account"
    },
    {
      "source": "/ally-admin/forms/auth",
      "destination": "api/ally-admin/forms/auth"
    },
    {
      "source": "/ally-admin/forms/create-account",
      "destination": "api/ally-admin/forms/create-account"
    },
    {
      "source": "/ally-admin/:path*",
      "destination": "api/ally-admin"
    },
    {
      "source": "/(.*)",
      "destination": "api/ally"
    }
  ],
  "headers": [
    {
      "source": "/ally-admin/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, must-revalidate, private"
        }
      ]
    }
  ]
}
